{
	"manifest_version": 2,
	
	"name": "Sangwidge",
	"description": "Replaces every instance of 'sandwich' with 'sangwidge'",
	"version": "0.1",
	"icons": { "128": "images/icon_128.png" },
	
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_end"
		}
	],

	"browser_action": {
		"default_icon": "images/icon_19.png"
	}
}

